{
  "name": "@dao-xyz/libp2p-noise",
  "version": "11.1.3",
  "author": "dao.xyz",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/dao-xyz/js-libp2p-noise#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dao-xyz/js-libp2p-noise.git"
  },
  "bugs": {
    "url": "https://github.com/dao-xyz/js-libp2p-noise/issues"
  },
  "keywords": [
    "crypto",
    "libp2p",
    "noise"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist/src",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "import": "./dist/src/index.js"
    }
  },
  "browser": {
    "./dist/src/alloc-unsafe.js": "./dist/src/alloc-unsafe-browser.js",
    "./dist/src/crypto/crypto-node.js": "./dist/src/crypto/crypto-browser.js",
    "util": false
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/explicit-function-return-type": "warn",
      "@typescript-eslint/strict-boolean-expressions": "off"
    },
    "ignorePatterns": [
      "src/proto/payload.js",
      "src/proto/payload.d.ts",
      "benchmarks",
      "test/fixtures/node-globals.js"
    ]
  },
  "scripts": {
    "bench": "npm run bench:handshake && npm run bench:nonce && npm run bench:crypto",
    "bench:handshake": "node --loader ts-node/esm ./benchmarks/handshake.ts",
    "bench:nonce": "node --loader ts-node/esm ./benchmarks/nonce.ts",
    "bench:crypto": "node --loader ts-node/esm ./benchmarks/crypto.ts",
    "clean": "aegir clean",
    "dep-check": "aegir dep-check",
    "build":"aegir build",
    "prebuild": "mkdirp dist/src && cp -R src/proto dist/src",
    "lint": "aegir lint",
    "lint:fix": "aegir lint --fix",
    "test": "aegir test",
    "test:node": "aegir test -t node",
    "test:browser": "aegir test -t browser -t webworker",
    "test:electron-main": "aegir test -t electron-main",
    "test:interop": "aegir test -t node -f dist/test/interop.js",
    "docs": "aegir docs",
    "proto:gen": "protons ./src/proto/payload.proto",
    "prepublish": "npm run build",
    "postinstall": "node -e \"let myLibAsDep = require('fs').existsSync('.git'); if (myLibAsDep) process.exit(1);\" || patch-package"
  },
  "dependencies": {
    "@libp2p/crypto": "^1.0.17",
    "@libp2p/interface-connection-encrypter": "^4.0.1",
    "@libp2p/interface-keys": "^1.0.8",
    "@libp2p/interface-metrics": "^4.0.8",
    "@libp2p/interface-peer-id": "^2.0.2",
    "@libp2p/logger": "^2.0.7",
    "@libp2p/peer-id": "^2.0.3",
    "@stablelib/hkdf": "^1.0.1",
    "@stablelib/sha256": "^1.0.1",
    "it-length-prefixed": "^9.0.1",
    "it-pair": "^2.0.6",
    "it-pb-stream": "^4.0.1",
    "it-pipe": "^3.0.1",
    "it-stream-types": "^2.0.1",
    "protons-runtime": "^5.0.0",
    "uint8arraylist": "^2.4.3",
    "uint8arrays": "^4.0.3",
    "libsodium-wrappers":"^0.7.11"
  },
  "devDependencies": {
    "@libp2p/daemon-client": "^6.0.0",
    "@libp2p/daemon-server": "^5.0.0",
    "@libp2p/interface-connection-encrypter-compliance-tests": "^5.0.1",
    "@libp2p/interop": "^8.0.1",
    "@libp2p/mplex": "^8.0.1",
    "@libp2p/peer-id-factory": "^2.0.3",
    "@libp2p/tcp": "^7.0.1",
    "@multiformats/multiaddr": "^12.1.2",
    "aegir": "^39.0.5",
    "benchmark": "^2.1.4",
    "execa": "^6.1.0",
    "go-libp2p": "^1.0.3",
    "iso-random-stream": "^2.0.2",
    "libp2p": "^0.45.0",
    "mkdirp": "^3.0.1",
    "p-defer": "^4.0.0",
    "protons": "^7.0.2",
    "sinon": "^15.0.4",
    "@types/sinon":"^10.0.14",
    "util": "^0.12.5",
    "ts-node":"^10.9.1",
    "@types/benchmark":"^2.1.2",
    "@types/libsodium-wrappers": "^0.7.9",
    "patch-package":"^6.5.1"
  }

}
