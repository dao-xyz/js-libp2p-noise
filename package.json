{
  "name": "@dao-xyz/libp2p-noise",
  "version": "11.0.0",
  "author": "dao.xyz",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/dao-xyz/js-libp2p-noise#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dao-xyz/js-libp2p-noise.git"
  },
  "bugs": {
    "url": "https://github.com/dao-xyz/js-libp2p-noise/issues"
  },
  "keywords": [
    "crypto",
    "libp2p",
    "noise"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist/src",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "import": "./dist/src/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/explicit-function-return-type": "warn",
      "@typescript-eslint/strict-boolean-expressions": "off"
    },
    "ignorePatterns": [
      "src/proto/payload.js",
      "src/proto/payload.d.ts",
      "test/fixtures/node-globals.js"
    ]
  },
  "scripts": {
    "bench": "npm run bench:handshake && npm run bench:nonce && npm run bench:crypto",
    "bench:handshake": "node --loader ts-node/esm ./benchmarks/handshake.ts",
    "bench:nonce": "node --loader ts-node/esm ./benchmarks/nonce.ts",
    "bench:crypto": "node --loader ts-node/esm ./benchmarks/crypto.ts",
    "clean": "aegir clean",
    "dep-check": "aegir dep-check",
    "build":"aegir build",
    "build2": "tsc -p tsconfig.json && esbuild  --format=esm --sourcemap --outfile=dist/index.min.js --bundle=dist/src/index.js --entry-names=dist/src/index.js --minify --define:'global'='globalThis' --define:'process.env.NODE_ENV'='\"production\"' --log-level=warning",
    "prebuild": "mkdirp dist/src && cp -R src/proto dist/src",
    "lint": "aegir lint",
    "lint:fix": "aegir lint --fix",
    "test": "aegir test",
    "test:node": "aegir test -t node",
    "test:browser": "aegir test -t browser -t webworker",
    "test:electron-main": "aegir test -t electron-main",
    "test:interop": "aegir test -t node -f dist/test/interop.js",
    "docs": "aegir docs",
    "proto:gen": "protons ./src/proto/payload.proto",
    "prepublish": "npm run build",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@libp2p/crypto": "^1.0.10",
    "@libp2p/interface-connection-encrypter": "^3.0.3",
    "@libp2p/interface-keys": "^1.0.6",
    "@libp2p/interface-metrics": "^4.0.4",
    "@libp2p/interface-peer-id": "^1.1.2",
    "@libp2p/logger": "^2.0.0",
    "@libp2p/peer-id": "^1.1.8",
    "@stablelib/hkdf": "^1.0.1",
    "@stablelib/sha256": "^1.0.1",
    "it-length-prefixed": "^8.0.2",
    "it-pair": "^2.0.3",
    "it-pb-stream": "^2.0.3",
    "it-pipe": "^2.0.5",
    "it-stream-types": "^1.0.5",
    "protons-runtime": "^4.0.1",
    "uint8arraylist": "^2.4.3",
    "uint8arrays": "^4.0.2",
    "libsodium-wrappers":"^0.7.10"
  },
  "devDependencies": {
    "@libp2p/daemon-client": "^3.0.1",
    "@libp2p/daemon-server": "^3.0.1",
    "@libp2p/interface-connection-encrypter-compliance-tests": "^3.0.0",
    "@libp2p/interop": "^3.0.1",
    "@libp2p/mplex": "^7.0.0",
    "@libp2p/peer-id-factory": "^1.0.8",
    "@libp2p/tcp": "^6.0.2",
    "@multiformats/multiaddr": "^11.0.3",
    "aegir": "^37.3.0",
    "benchmark": "^2.1.4",
    "execa": "^6.1.0",
    "go-libp2p": "^0.0.6",
    "iso-random-stream": "^2.0.2",
    "libp2p": "^0.41.0",
    "mkdirp": "^1.0.4",
    "p-defer": "^4.0.0",
    "protons": "^6.0.0",
    "sinon": "^15.0.0",
    "util": "^0.12.4",
    "ts-node":"^10.9.1",
    "@types/benchmark":"^2.1.2",
    "@types/libsodium-wrappers": "^0.7.9",
    "patch-package":"^6.5.1"
  },
  "browser": {
    "./dist/src/alloc-unsafe.js": "./dist/src/alloc-unsafe-browser.js",
    "util": false
  }
}
